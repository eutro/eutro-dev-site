{
  "name": "eutro-dev-site",
  "version": "1.0.0",
  "description": "Home to Eutro and its things.",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build-ssr": "vite build --outDir dist/server --ssr src/entry-server.tsx && node ./precompile.js",
